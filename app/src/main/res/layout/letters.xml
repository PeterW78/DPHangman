<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <Button
        android:id="@+id/a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/A" />
    <Button
        android:id="@+id/b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/B"
        android:layout_toRightOf="@+id/a"/>
    <Button
        android:id="@+id/c"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/C"
        android:layout_toRightOf="@+id/b"/>
    <Button
        android:id="@+id/d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/D"
        android:layout_toRightOf="@+id/c"/>
    <Button
        android:id="@+id/e"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/E"
        android:layout_toRightOf="@+id/d"/>
    <Button
        android:id="@+id/f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/F"
        android:layout_below="@+id/a" />

    <Button
        android:id="@+id/g"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/G"
        android:layout_below="@+id/b"
        android:layout_toRightOf="@+id/f"/>
    <Button
        android:id="@+id/h"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/H"
        android:layout_below="@+id/c"
        android:layout_toRightOf="@+id/g"/>

    <Button
        android:id="@+id/i"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/I"
        android:layout_below="@+id/d"
        android:layout_toRightOf="@+id/h"/>
    <Button
        android:id="@+id/j"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/J"
        android:layout_below="@+id/e"
        android:layout_toRightOf="@+id/i"/>
    <Button
        android:id="@+id/k"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/K"
        android:layout_below="@+id/f" />
    <Button
        android:id="@+id/l"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/L"
        android:layout_below="@+id/g"
        android:layout_toRightOf="@+id/k"/>
    <Button
        android:id="@+id/m"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/M"
        android:layout_below="@+id/h"
        android:layout_toRightOf="@+id/l"/>
    <Button
        android:id="@+id/n"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/N"
        android:layout_below="@+id/i"
        android:layout_toRightOf="@+id/m"/>
    <Button
        android:id="@+id/o"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/O"
        android:layout_below="@+id/j"
        android:layout_toRightOf="@+id/n"/>
    <Button
        android:id="@+id/p"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/P"
        android:layout_below="@+id/k" />
    <Button
        android:id="@+id/q"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Q"
        android:layout_below="@+id/l"
        android:layout_toRightOf="@+id/p"/>
    <Button
        android:id="@+id/r"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/R"
        android:layout_below="@+id/m"
        android:layout_toRightOf="@+id/q"/>
    <Button
        android:id="@+id/s"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/S"
        android:layout_below="@+id/n"
        android:layout_toRightOf="@+id/r"/>
    <Button
        android:id="@+id/t"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/T"
        android:layout_below="@+id/o"
        android:layout_toRightOf="@+id/s"/>
    <Button
        android:id="@+id/u"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/U"
        android:layout_below="@+id/p"/>
    <Button
        android:id="@+id/v"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/V"
        android:layout_below="@+id/q"
        android:layout_toRightOf="@+id/u"/>
    <Button
        android:id="@+id/w"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/W"
        android:layout_below="@+id/r"
        android:layout_toRightOf="@+id/v"/>
    <Button
        android:id="@+id/x"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/X"
        android:layout_below="@+id/s"
        android:layout_toRightOf="@+id/w"/>
    <Button
        android:id="@+id/y"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Y"
        android:layout_below="@+id/t"
        android:layout_toRightOf="@+id/x"/>
    <Button
        android:id="@+id/z"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Z"
        android:layout_below="@+id/u"
        android:layout_toRightOf="@+id/g"/>

</RelativeLayout>